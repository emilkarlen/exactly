#
# The "run" instruction can be used in all phases except "act".
#

[setup]


run my-setup-helper-program with arguments

run /usr/bin/python --interpret my-py-setup-helper.py 'with argument'

run ( /usr/bin/python -c ) --source print('This does some setup!')


[act]


system-under-test


[assert]


#
# Assertion passes if exit code from the program is 0.
#

run my-assert-helper-program
