[setup]

file 2.txt =

    -stdout-from
        bin/print-one-argument-per-line 0 1 2 3 4 5 6 7
    -transformed-by
        run -python -existing-file -rel-home bin/filter-line-num-multiple-of.py 2

file 3.txt =

    -stdout-from
        bin/print-one-argument-per-line 0 1 2 3 4 5 6 7
    -transformed-by
        run -python -existing-file -rel-home bin/filter-line-num-multiple-of.py 3


[assert]

contents 2.txt

    equals <<-
0
2
4
6
-

contents 3.txt

    equals <<-
0
3
6
-
