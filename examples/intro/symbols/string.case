[setup]


def string GOOD = 'very good'

def string MY_FILE = my-file.txt

file @[MY_FILE]@ = <<EOF
a @[GOOD]@ 1st line
a bad 2nd line
a @[GOOD]@ 3d line
a line we don't want
EOF


[act]


filter-lines @[MY_FILE]@ @[GOOD]@


[assert]


contents @[MY_FILE]@ equals <<EOF
a @[GOOD]@ 1st line
a @[GOOD]@ 3d line
EOF


[conf]


act-home = bin
