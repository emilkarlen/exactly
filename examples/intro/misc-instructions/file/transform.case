[setup]


file bike-ratings.txt = <<EOF
ok    Bianchi
good  BMC
ok    Cannondale
great de Rosa
good  Cube
great Lapierre
EOF


[before-assert]


file great-bikes.txt  = --transformed-by select regex '^great'
                        --file bike-ratings.txt

[assert]


contents  great-bikes.txt  num-lines == 2
