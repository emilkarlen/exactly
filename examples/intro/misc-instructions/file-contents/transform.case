[setup]


file bike-ratings.txt = <<EOF
ok    Bianchi
good  BMC
ok    Cannondale
great de Rosa
good  Cube
great Lapierre
EOF


[before-assert]


file great-bikes.txt  = -file bike-ratings.txt
                        -transformed-by filter matches '^great'

[assert]


contents  great-bikes.txt  num-lines == 2
