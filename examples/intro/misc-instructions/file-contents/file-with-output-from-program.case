[setup]

file my-fst-file.txt = -stdout-from
                       -python -existing-file print-arguments.py "Hello, Python World!"

file my-snd-file.txt = -stdout-from
                       -python -existing-file print-arguments.py :> Hello, Python World!

[assert]

contents my-fst-file.txt equals <<eof
Hello, Python World!
eof

contents my-snd-file.txt equals <<eof
Hello, Python World!
eof
