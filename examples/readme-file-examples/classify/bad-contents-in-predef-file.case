[setup]

dir input
dir output/good
dir output/bad

file input/a.txt = <<EOF
GOOD contents
EOF

file input/b.txt = -file -rel-home predefined-bad-contents-file.txt

[act]

classify-files-by-moving-to-appropriate-dir GOOD input/ output/

[assert]

dir-contents input        empty

dir-contents output/good  equals { a.txt : type file }

dir-contents output/bad   equals { b.txt : type file }
