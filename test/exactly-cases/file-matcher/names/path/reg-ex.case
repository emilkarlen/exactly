[setup]

file abc.txt
file xyz.txt

[assert]

dir-contents .
             -selection path ~ act.*abc
             num-files == 1

def string AB_REGEX = ab

dir-contents .
             -selection path ~ @[AB_REGEX]@c
             num-files == 1

def file-matcher PATH_AB_REGEX = path ~ @[AB_REGEX]@c

dir-contents .
             -selection PATH_AB_REGEX
             num-files == 1
