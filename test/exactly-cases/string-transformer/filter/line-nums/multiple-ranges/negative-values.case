[setup]

file f.txt = <<-
1
2
3
4
5
-

[assert]


contents f.txt
  -transformed-by
    filter -line-nums -1 -4
    equals
<<-
2
5
-

contents f.txt
  -transformed-by
    filter -line-nums -2 -3 -5
    equals
<<-
1
3
4
-

contents f.txt
  -transformed-by
    filter -line-nums 1 -3:-2
    equals
<<-
1
3
4
-

contents f.txt
  -transformed-by
    filter -line-nums :2 -2:
    equals
<<-
1
2
4
5
-

contents f.txt
  -transformed-by
    filter -line-nums -10: -2:
    equals
<<-
1
2
3
4
5
-

contents f.txt
  -transformed-by
    filter -line-nums -5:-4 -2:10
    equals
<<-
1
2
4
5
-
