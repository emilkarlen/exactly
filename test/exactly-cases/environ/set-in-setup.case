[setup]

def string VAR_VAL = 'the value of the env var'

env SETUP_VAR = @[VAR_VAL]@

run @ ENV_VAR_EQUALS SETUP_VAR @[VAR_VAL]@

[act]

@ PRINT_ENV_VAR SETUP_VAR

[before-assert]

run @ ENV_VAR_EQUALS SETUP_VAR @[VAR_VAL]@

[assert]

stdout equals @[VAR_VAL]@

run @ ENV_VAR_EQUALS SETUP_VAR @[VAR_VAL]@

[cleanup]

run @ ENV_VAR_EQUALS SETUP_VAR @[VAR_VAL]@
