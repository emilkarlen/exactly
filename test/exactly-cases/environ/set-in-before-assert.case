[setup]

run @ ENV_VAR_IS_NOT_SET BEFORE_ASSERT_VAR

[before-assert]


def string VAR_VAL = 'the value of the before-assert env var'

env BEFORE_ASSERT_VAR = @[VAR_VAL]@

run @ ENV_VAR_EQUALS BEFORE_ASSERT_VAR @[VAR_VAL]@


[assert]


run @ ENV_VAR_EQUALS BEFORE_ASSERT_VAR @[VAR_VAL]@


[cleanup]


run @ ENV_VAR_EQUALS BEFORE_ASSERT_VAR @[VAR_VAL]@
